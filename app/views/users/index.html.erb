<div class="container">
  <h3>Users</h3>
  <div class="column">
    <table class="table">
    	<thead>
        <th> user id </th>
        <th colspan="3"></th>
    		<th> Email </th>
        <th colspan="3"></th>
        <th> User Name </th>
        <th colspan="3"></th>
    	</thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <%= render user %>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>